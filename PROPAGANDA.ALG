Algoritmo "Propaganda"

Var
// Seção de Declarações das variáveis 
    produto1 : caractere
    produto2 : caractere
    preco1 : real
    preco2 : real
    idade : inteiro
    codigo : inteiro
    genero : caractere

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
    produto1 <- "Computador"
    produto2 <- "TV"
    preco1 <- 2100.5
    preco2 <- 1830.0
    idade <- 30
    codigo <- 5291
    genero <- "F"

    escreval("Produtos:")
    escreval("O Produto " , produto1, " custa " , preco1:8:2)
    escreval("Produtos: " , produto2 , " custa " , preco2)
    escreval
    escreval("Codigo: ", codigo)
    escreval
    escreval("Dados da pessoa:" , " Genero: ", genero, " e " ,"Idade: ", idade)


    escreval()
Fimalgoritmo